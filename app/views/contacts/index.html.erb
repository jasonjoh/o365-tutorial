<h1>My contacts</h1>
<table class="table">
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
  </tr>
  <% @contacts.each do |contact| %>
    <tr>
      <td><%= contact.given_name %></td>
      <td><%= contact.surname %></td>
      <td><%= contact.email_addresses[0].address %></td>
    </tr>
  <% end %>
</table>